{
  "name": "@morgus/mcp-server",
  "version": "1.0.0",
  "description": "MCP server for exporting Morgys to Claude Desktop and other platforms",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "morgy-mcp": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "inspector": "npx @modelcontextprotocol/inspector dist/index.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "morgy",
    "ai-agent",
    "claude-desktop"
  ],
  "author": "Morgus",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@supabase/supabase-js": "^2.39.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  }
}
